{% extends 'base.html.twig' %}

{% block title %}Ct404Category index{% endblock %}

{% block body %}
    <h1>Ct404Category index</h1>

    <table class="table">
        <thead>
            <tr>
                <th>Id</th>
                <th>actions</th>
            </tr>
        </thead>
        <tbody>
        {% for ct404_category in ct404_categories %}
            <tr>
                <td>{{ ct404_category.id }}</td>
                <td>
                    <a href="{{ path('ct404_category_show', {'id': ct404_category.id}) }}">show</a>
                    <a href="{{ path('ct404_category_edit', {'id': ct404_category.id}) }}">edit</a>
                </td>
            </tr>
        {% else %}
            <tr>
                <td colspan="2">no records found</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <a href="{{ path('ct404_category_new') }}">Create new</a>
{% endblock %}
