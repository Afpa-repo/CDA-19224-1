{% extends 'base.html.twig' %}

{% block title %}Ct404OrderDetail index{% endblock %}

{% block body %}
    <h1>Ct404OrderDetail index</h1>

    <table class="table">
        <thead>
            <tr>
                <th>Id</th>
                <th>Quantity</th>
                <th>actions</th>
            </tr>
        </thead>
        <tbody>
        {% for ct404_order_detail in ct404_order_details %}
            <tr>
                <td>{{ ct404_order_detail.id }}</td>
                <td>{{ ct404_order_detail.quantity }}</td>
                <td>
                    <a href="{{ path('ct404_order_detail_show', {'id': ct404_order_detail.id}) }}">show</a>
                    <a href="{{ path('ct404_order_detail_edit', {'id': ct404_order_detail.id}) }}">edit</a>
                </td>
            </tr>
        {% else %}
            <tr>
                <td colspan="3">no records found</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <a href="{{ path('ct404_order_detail_new') }}">Create new</a>
{% endblock %}
